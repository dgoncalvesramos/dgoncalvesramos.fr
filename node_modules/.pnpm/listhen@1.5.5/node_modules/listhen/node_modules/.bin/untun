#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/david/Documents/dgoncalvesramos.fr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/david/Documents/dgoncalvesramos.fr/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../untun@0.1.2/node_modules/untun/bin/untun.mjs" "$@"
else
  exec node  "$basedir/../../../../../untun@0.1.2/node_modules/untun/bin/untun.mjs" "$@"
fi
